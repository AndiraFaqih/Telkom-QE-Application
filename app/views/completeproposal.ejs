<form>
    <div class="flex flex-col">
        <div class="overflow-x-auto sm:-mx-6 lg:-mx-8">
            <div class="py-2 inline-block min-w-full sm:px-6 lg:px-8">
                <div class="overflow-x-auto">
                    <table class="min-w-full">
                    <thead class="border-b bg-gray-800 text-white">
                        <tr>
                        <th scope="col" class="text-sm font-semibold uppercase px-6 py-4 text-center">ID Project</th>
                        <th scope="col" class="text-sm font-semibold uppercase px-0 py-4 text-center">Nama STO</th>
                        <th scope="col" class="text-sm font-semibold uppercase px-6 py-4 text-center">Segmen</th>
                        <th scope="col" class="text-sm font-semibold uppercase px-6 py-4 text-center">Nama Alpro</th>
                        <th scope="col" class="text-sm font-semibold uppercase px-6 py-4 text-center">jenisQE</th>
                        <th scope="col" class="text-sm font-semibold uppercase px-6 py-4 text-center">status</th>
                        <th scope="col" class="text-sm font-semibold uppercase px-6 py-4 text-center">Action</th>
                        </tr>
                    </thead>

                    <tbody id="tabel" class="text-gray-700">
                        <% proposals.forEach(function(proposals) { 
                            if (proposals.design && proposals.rab) { %>
                            <tr class="border-b">
                                <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap text-center"><%= proposals.idProposal %></td>
                                <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap text-center"><%= proposals.namaSTO %></td>
                                <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap text-center"><%= proposals.segmen %></td>
                                <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap text-center"><%= proposals.namaAlpro %></td>
                                <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap text-center"><%= proposals.jenisQE %></td>             
                                <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap text-center"><%= proposals.status %></td>             
                                <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap text-center">
                                    <a
                                        type="button"
                                        class="inline-block px-6 py-2.5 bg-blue-400 text-white font-medium text-xs leading-tight uppercase rounded shadow-md hover:bg-blue-500 hover:shadow-lg focus:bg-blue-500 focus:shadow-lg focus:outline-none focus:ring-0 active:bg-blue-600 active:shadow-lg transition duration-150 ease-in-out"
                                        href="/detail/<%= proposals._id %>/design"
                                        >Detail
                                    </a>    
                                </td>
                            </tr>
                            <% } %>
                        <% }); %>
                    </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</form>
  