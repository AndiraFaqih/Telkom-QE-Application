<div class="flex flex-col">
    <div class="overflow-x-auto sm:-mx-6 lg:-mx-8">
      <div class="py-2 inline-block min-w-full sm:px-6 lg:px-8">
        <div class="overflow-hidden">
          <table class="min-w-full text-center">
            <thead class="border-b">
                <tr class="border-b bg-gray-800 boder-gray-900">
                    <td class="text-sm text-white font-medium px-6 py-4 whitespace-nowrap">
                        ID Proposal
                    </td>
                    <td class="text-sm text-white font-light px-6 py-4 whitespace-nowrap">
                        Nama STO
                    </td>
                    <td class="text-sm text-white font-light px-6 py-4 whitespace-nowrap">
                        Segmen
                    </td>
                    <td class="text-sm text-white font-light px-6 py-4 whitespace-nowrap">
                        Nama alpro
                    </td>
                    <td class="text-sm text-white font-light px-6 py-4 whitespace-nowrap">
                        Jenis QE
                    </td>
                    <td class="text-sm text-white font-light px-6 py-4 whitespace-nowrap">
                        Koordinat ODP Baru
                    </td>
                    <td class="text-sm text-white font-light px-6 py-4 whitespace-nowrap">
                        Keterangan
                    </td>
                    <td class="text-sm text-white font-light px-6 py-4 whitespace-nowrap">
                        status
                    </td>
                </tr>
            </thead>
            <tbody>
                <% proposals.forEach(function(proposals) { %>
                    <tr class="border-b bg-gray-50 border-gray-200">
                        <td class="text-sm text-gray-900 font-medium px-6 py-4 whitespace-nowrap">
                            <%= proposals.idProposal %> 
                        </td>
                        <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap">
                            <%= proposals.namaSTO %> 
                        </td>
                        <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap">
                            <%= proposals.segmen %> 
                        </td>
                        <td class="text-sm text-gray-900 font-medium px-6 py-4 whitespace-nowrap">
                            <%= proposals.namaAlpro %> 
                        </td>
                        <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap">
                            <%= proposals.jenisQE %> 
                        </td>
                        <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap">
                            <%= proposals.koordinatODPBaru %> 
                        </td>
                        <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap">
                            <%= proposals.keterangan %> 
                        </td>
                        <td class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap">
                            <%= proposals.status %> 
                        </td>
                    </tr>
                 <% }) %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

  <%- include('./js/js.ejs') %>
