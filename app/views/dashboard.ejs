<div class="flex flex-col">
  <div class="overflow-x-auto sm:-mx-6 lg:-mx-8">
    <div class="py-2 inline-block min-w-full sm:px-6 lg:px-8">
      <div class="overflow-hidden">
        <table class="min-w-full text-center">
          <thead class="border-b">
            <tr class="border-b bg-gray-800 boder-gray-900">
              <td
                class="text-sm text-white font-medium px-6 py-4 whitespace-nowrap"
              >
                ID Proposal
              </td>
              <td
                class="text-sm text-white font-light px-6 py-4 whitespace-nowrap"
              >
                Nama STO
              </td>
              <td
                class="text-sm text-white font-light px-6 py-4 whitespace-nowrap"
              >
                Segmen
              </td>
              <td
                class="text-sm text-white font-light px-6 py-4 whitespace-nowrap"
              >
                Nama alpro
              </td>
              <td
                class="text-sm text-white font-light px-6 py-4 whitespace-nowrap"
              >
                Jenis QE
              </td>
              <td
                class="text-sm text-white font-light px-6 py-4 whitespace-nowrap"
              >
                Koordinat ODP Baru
              </td>
              <td
                class="text-sm text-white font-light px-6 py-4 whitespace-nowrap"
              >
                Keterangan
              </td>
              <td
                class="text-sm text-white font-light px-6 py-4 whitespace-nowrap"
              >
                status
              </td>
            </tr>
          </thead>
          <tbody>
            <% proposal.forEach(function(proposal) { %>
            <tr class="border-b bg-gray-50 border-gray-200">
              <td
                class="text-sm text-gray-900 font-medium px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.idProposal %>
              </td>
              <td
                class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.namaSTO %>
              </td>
              <td
                class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.segmen %>
              </td>
              <td
                class="text-sm text-gray-900 font-medium px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.namaAlpro %>
              </td>
              <td
                class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.jenisQE %>
              </td>
              <td
                class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.koordinatODPBaru %>
              </td>
              <td
                class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.keterangan %>
              </td>
              <td
                class="text-sm text-gray-900 font-light px-6 py-4 whitespace-nowrap"
              >
                <%= proposal.status %>
              </td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<%- include('./js/js.ejs') %>
