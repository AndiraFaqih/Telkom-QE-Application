<form>
  <p class="text-xl pb-3 flex items-center ml-1">
    <i class="fas fa-list mr-3"></i> New Proposal
  </p>
  <div class="max-w-7xl h-auto rounded overflow-hidden shadow-lg">
    <div class="flex flex-col">
      <div class="overflow-x-auto sm:-mx-6 lg:-mx-8">
        <div class=" min-w-full sm:px-6 lg:px-8">
          <div class="overflow-hidden">
            <table class="min-w-full text-center" style=" width: 150px;">
              <thead class="bg-gray-800 text-white">
                <tr>
                  <th
                    class="text-center py-3 px-4 uppercase font-semibold text-sm"
                  >
                    ID Project
                  </th>
                  <th
                    class="text-center py-3 px-4 uppercase font-semibold text-sm"
                  >
                    Nama STO
                  </th>
                  <th
                    class="text-center py-3 px-4 uppercase font-semibold text-sm"
                  >
                    Segmen
                  </th>
                  <th
                    class="text-center py-3 px-4 uppercase font-semibold text-sm"
                  >
                    nama Alpro 
                  </th>
                  <th
                    class="text-center py-3 px-4 uppercase font-semibold text-sm"
                  >
                    Jenis QE
                  </th>
                  <th
                    class="text-center py-3 px-4 uppercase font-semibold text-sm"
                  >
                    Action
                  </th>
                </tr>
              </thead>
              <tbody>
                <% proposal.forEach(function (proposal, index) { if (!proposal.design && !proposal.rab && proposal.status == "SUBMITTED")  { %>
                  <tr class="<%= index % 2 == 0 ? 'bg-gray-100 border-b' : 'bg-white border-b' %> text-center">
                    <td class="text-center py-3 px-4"><%= proposal.idProposal %></td>
                    <td class="text-center py-3 px-4"><%= proposal.namaSTO %></td>
                    <td class="text-center py-3 px-4"><%= proposal.segmen %></td>
                    <td class="text-center py-3 px-4"><%= proposal.namaAlpro %></td>
                    <td class="text-center py-3 px-4"><%= proposal.jenisQE %></td>
    
                    <td class="text-center py-3 px-4">
                      <a
                        type="button"
                        class="inline-block px-6 py-2.5 bg-blue-400 text-white font-medium text-xs leading-tight uppercase rounded shadow-md hover:bg-blue-500 hover:shadow-lg focus:bg-blue-500 focus:shadow-lg focus:outline-none focus:ring-0 active:bg-blue-600 active:shadow-lg transition duration-150 ease-in-out"
                        href="/detail/<%= proposal._id %>/design"
                        >Detail</a
                      >
                    
                    </td>
                  </tr>
                  <% } %> 
                <% }); %>
              </tbody>
            </table>
          </div>
        </div>
        </div>
      </div>
    </div>
</form>
