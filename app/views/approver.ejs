<form>
  <div class="w-full mt-8">
    <p class="text-xl pb-3 flex items-center">
      <i class="fas fa-list mr-3"></i> Submitted Proposals
    </p>
    <div class="bg-white overflow-auto">
      <table class="min-w-full bg-white">
        <thead class="bg-gray-800 text-white">
          <tr>
            
            <th
              class="text-center py-3 px-4 uppercase font-semibold text-sm"
            >
              ID Project
            </th>
            <th
              class="text-center py-3 px-4 uppercase font-semibold text-sm">Nama STO</th>
              <th class="text-center py-3 px-4 uppercase font-semibold text-sm">Segmen</th>
                <th class="text-center py-3 px-4 uppercase font-semibold text-sm">Nama Alpro</th>
                <th class="text-center py-3 px-4 uppercase font-semibold text-sm">jenisQE</th>
                <th class="text-center py-3 px-4 uppercase font-semibold text-sm">Action</th>
            
          </tr>
        </thead>
        <tbody id="tabel" class="text-gray-700">
             <!--if proposal.proposer exist in proposer then return proposer.name  -->
              
              
              <% proposals.forEach(function(proposals) { if (proposals.design && proposals.rab) { %>

                <td class="text-center py-3 px-4"><%= proposals.idProposal %></td>
                <td class="text-center py-3 px-4"><%= proposals.namaSTO %></td>
                <td class="text-center py-3 px-4"><%= proposals.segmen %></td>
                <td class="text-center py-3 px-4"><%= proposals.namaAlpro %></td>
                <td class="text-center py-3 px-4"><%= proposals.jenisQE %></td>
                <td class="text-center py-3 px-4">
                  <a
                    type="button"
                    class="inline-block px-6 py-2.5 bg-blue-400 text-white font-medium text-xs leading-tight uppercase rounded shadow-md hover:bg-blue-500 hover:shadow-lg focus:bg-blue-500 focus:shadow-lg focus:outline-none focus:ring-0 active:bg-blue-600 active:shadow-lg transition duration-150 ease-in-out"
                    href="/detail/<%= proposals._id %>/approver"
                    >Detail</a
                  >               

              </tr>
              <% } %>
              <% }); %>
        </tbody>
      </table>
    </div>
    <!-- button to show the next data  -->
    
    
  </div>
</form>
